//         document.getElementById('product').style.backgroundColor = "beige"
        function handleResizeHeight(textarea) {
            textarea.style.height = 'auto'; //height 초기화
            textarea.style.height = textarea.scrollHeight + 'px';
            let temp = document.getElementById("jsonData").value;
            // let jsonData = document.getElementByID("jsonData");
            let myJsonData = JSON.parse(temp);
            console.log(myJsonData);
        }

        window.onload = () => {
          const panelFaqContainer = document.querySelectorAll("#collapse_btn_field"); // NodeList 객체

          // panel-faq-answer
          let panelFaqAnswer = document.querySelectorAll(".accordion-class");

          let product_no_field = document.querySelectorAll(".product_no_field");


          // btn-all-close
          const btnAllClose = document.querySelector("#btn-all-close");

          // 반복문 순회하면서 해당 제목 클릭시 콜백 처리
          for( let i=0; i < panelFaqContainer.length; i++ ) {
            panelFaqContainer[i].addEventListener('click', function() { // 클릭시 처리할 일
              // FAQ 제목 클릭시 -> 본문이 보이게끔 -> active 클래스 추가
              panelFaqAnswer[i].classList.toggle('active');
              console.log("bbb");
            });
          };
        }

        function modifyProduct(product_no) {
            let submit_name = `modify_${product_no}`;
            if(confirm("제품 정보를 수정합니다.") === true) {
                document[submit_name].submit();
            }
            else {
                return false;
            }
        }

        function delProduct(product_no) {
            if(confirm("제품 정보를 삭제할까요?") === true) {
                location.href = `/api/item/del?product_no=${product_no}`;
            }
            else {
                return false;
            }
        }
    </script>

